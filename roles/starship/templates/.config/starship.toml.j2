# {{ ansible_managed }}

add_newline = false

[hostname]
ssh_only = false

[username]
show_always = true

[directory]
truncate_to_repo =false
truncation_length=15

[terraform]
disabled = true

[kubernetes]
disabled = false

[nodejs]
disabled = true

[java]
disabled = true

[python]
python_binary = 'python3'

[[battery.display]]
threshold = 20
style = "bold red"
